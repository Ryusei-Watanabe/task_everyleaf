<div class="wrapper">
  <p id="notice"><%= notice %></p>
  <h1>Task List</h1>
  <% @task.each do |task| %>
    <div class="article_wrapper">
      <%= link_to task do %>
        <div class="article">
          <div class="title_wrapper">Title: <%= task.title %></div>
          <div class="content_wrapper">Content: <%= task.content %></div>
        </div>
      <% end %>
    </div>
  <% end %>
  <br>
  <%= link_to 'New task', new_task_path, class:"task-btn" %>
</div>
